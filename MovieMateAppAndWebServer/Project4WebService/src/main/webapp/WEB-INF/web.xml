<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

    <servlet>
        <servlet-name>MovieMateServlet</servlet-name>
        <servlet-class>ds.MovieMateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MovieMateServlet</servlet-name>
        <url-pattern>/MovieMateServlet/*</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>MovieMateServlet</welcome-file>
    </welcome-file-list>
</web-app>
